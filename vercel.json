{
  "version": 2,
  "builds": [
    {
    "src": "/index.js",
    "use": "@vercel/node"
    }
  ],
  "routes": [
    {
     "src": "/(.*)",
     "dest": "/"
    },
    {
     "src": "/login",
     "dest": "/routes/router.js"
    },
    {
     "src": "/cadastro",
     "dest": "/routes/router.js"
    },
    {
     "src": "/redefinir/senha",
     "dest": "/routes/router.js"
    },
    {
     "src": "/resetar-senha",
     "dest": "/routes/router.js"
    },
    {
     "src": "/dashboard",
     "dest": "/routes/router.js"
    }
    
  ]
}
